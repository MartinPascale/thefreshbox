)}\r\n          className={classNames(\"catalogo-list__category__2\", {\r\n            \"catalogo-list__category--grid\": showVegetable,\r\n          })}\r\n        >\r\n          {showVegetable ? (\r\n            vegetables.map((vegetable) => (\r\n              <ProductCard item={vegetable} handleAddToCart={handleAddToCart} />\r\n            ))\r\n          ) : (\r\n            <div>Verdura</div>\r\n          )}\r\n        </div>\r\n      </div>\r\n      {addedToCart && <BottomBar isCatalogo items={addedItems} link=\"/cart\" />}\r\n    </>\r\n  );\r\n};\r\n\r\nexport default Catalogo;\r\n","C:\\Users\\msi gs65\\projects\\thefreshbox\\src\\containers\\Checkout.js",["110","111","112","113","114"],"C:\\Users\\msi gs65\\projects\\thefreshbox\\src\\components\\BoxList.js",["115"],"import React, { useEffect } from 'react';\r\nimport { useDispatch, useSelector } from 'react-redux';\r\n\r\nimport fetchBoxes from '../helpers/fetchBoxes';\r\nimport BoxCard from './BoxCard';\r\n\r\nimport '../styles/BoxList.scss';\r\n\r\nconst BoxList = ({ title, refProp }) => {\r\n  const dispatch = useDispatch();\r\n\r\n  const list = useSelector((state) => state.toJS().boxes);\r\n\r\n  useEffect(() => {\r\n    fetchBoxes(dispatch);\r\n  }, []);\r\n\r\n  return (\r\n    <>\r\n      <div className='list-title'>{title}</div>\r\n      <div ref={refProp} className='list-container'>\r\n        {list.map((box) => (\r\n          <BoxCard box={box} />\r\n        ))}\r\n      </div>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default BoxList;\r\n","C:\\Users\\msi gs65\\projects\\thefreshbox\\src\\components\\Contacto.js",["116","117","118"],"import React from \"react\";\r\n\r\nimport ContactForm from \"./ContactForm\";\r\n\r\nimport logo from \"../assets/logo2.png\";\r\nimport whatsapp from \"../assets/whatsapp.png\";\r\nimport instagram from \"../assets/instagram.png\";\r\n\r\nimport \"../styles/Contacto.scss\";\r\n\r\nconst Contacto = (props) => {\r\n  const title = \"Contacto\";\r\n  return (\r\n    <